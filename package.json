    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^24.0.25",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/katex": "^0.11.0",
    "@types/morgan": "^1.9.1",
