generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model alembic_version {
  version_num String @id
}

model notes {
  id          Int     @default(autoincrement()) @id
  title       String?
  description String?
  user_id     Int?
}

model users {
  id       Int      @default(autoincrement()) @id
  username String?  @unique
  email    String?  @unique
  is_admin Boolean?
  password String?
}
